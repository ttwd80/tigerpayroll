<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC 
    "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN"
    "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd">
<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="INTEGER" hibernate-type="java.lang.Integer" />
		<sql-type jdbc-type="TIMESTAMP" hibernate-type="org.joda.time.DateTime" />
	</type-mapping>


	<table name="user_role" schema="public">
		<primary-key>
			<generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
				<param name="optimizer">none</param>
				<param name="increment_size">1</param>
				<param name="sequence_name">user_role_id_seq</param>
			</generator>
		</primary-key>
	</table>
	<table name="image" schema="public">
		<primary-key>
			<generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
				<param name="optimizer">none</param>
				<param name="increment_size">1</param>
				<param name="sequence_name">image_id_seq</param>
			</generator>
		</primary-key>
	</table>
</hibernate-reverse-engineering>
